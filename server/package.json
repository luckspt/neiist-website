{
  "name": "neiist-website-server",
  "scripts": {
    "start": "ts-node -r dotenv/config src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "build": "tsc",
    "prod": "node -r dotenv/config dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "axios": "1.8.4",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "exceljs": "^4.4.0",
    "express": "5.1.0",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.18.1",
    "htmlparser2": "10.0.0",
    "morgan": "1.10.0",
    "natural": "8.0.1",
    "pg": "8.14.1",
    "pg-error-enum": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-session": "^1.18.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.11",
    "eslint": "^9.24.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
